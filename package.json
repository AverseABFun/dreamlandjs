{
    "name": "dreamland",
    "version": "0.0.6",
    "description": "A utilitarian HTML rendering library",
    "scripts": {
        "build": "rollup -c && prettier -w .",
        "watch": "rollup -cw",
        "publish": "npm publish --access public"
    },
    "keywords": [
        "html",
        "jsx",
        "framework",
        "dreamlandjs",
        "dreamland"
    ],
    "author": "MercuryWorkshop",
    "repository": "https://github.com/MercuryWorkshop/dreamlandjs",
    "license": "MIT",
    "browser": "./dist/dev/index.js",
    "types": "./dreamland.d.ts",
    "node": "./dist/dev/index.js",
    "exports": {
        "./dev": {
            "default": "./dist/dev/index.js",
            "types": "./dreamland.d.ts"
        },
        "./js": {
            "default": "./dist/js.js",
            "types": "./dreamland.d.ts"
        },
        "./css": {
            "default": "./dist/css.js",
            "types": "./dreamland.d.ts"
        },
        "./html": {
            "default": "./dist/html.js",
            "types": "./dreamland.d.ts"
        },
        "./store": {
            "default": "./dist/store.js",
            "types": "./dreamland.d.ts"
        }
    },
    "files": [
        "dist",
        "dreamland.d.ts"
    ],
    "devDependencies": {
        "@rollup/plugin-strip": "^3.0.4",
        "@rollup/plugin-terser": "^0.4.4",
        "prettier": "^3.2.5",
        "rollup": "^4.13.0",
        "typescript": "^5.4.2"
    }
}
