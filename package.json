{
  "name": "dreamland",
  "version": "0.0.1",
  "description": "A utilitarian HTML rendering library",
  "scripts": {
    "build": "esbuild --minify --bundle DreamlandJS.js --outfile=index.js && esbuild --minify --bundle css.js js.js html.js --outdir=dist/ && tsc",
    "watch": "esbuild --watch=forever --bundle DreamlandJS.js --outfile=index.js & tsc --watch"
  },
  "keywords": [
    "html",
    "jsx",
    "framework",
    "dreamlandjs",
    "dreamland"
  ],
  "author": "MercuryWorkshop",
  "repository": "https://github.com/MercuryWorkshop/dreamlandjs",
  "license": "MIT",
  "browser": "./index.js",
  "node": "./index.js",
  "types": "./DreamlandJS.d.ts",
  "devDependencies": {
    "esbuild": "^0.19.11",
    "google-closure-compiler": "^20231112.0.0",
    "typescript": "^5.3.3"
  }
}
